{
   "name": "api.template.node",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "scripts": {
      "dev": "ts-node-dev --respawn --transpile-only --exit-child --watch src --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
      "build": "tsc && tsc-alias",
      "start": "node dist/server.js",
      "lint": "eslint . --ext .ts",
      "lint:fix": "eslint . --ext .ts --fix",
      "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
      "test": "vitest run",
      "test:watch": "vitest",
      "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "test-redis": "node -r tsconfig-paths/register -r ts-node/register src/test-redis.ts"
   },
   "dependencies": {
      "@types/compression": "^1.7.5",
      "@types/cookie-parser": "^1.4.8",
      "@types/morgan": "^1.9.9",
      "axios": "^1.8.4",
      "bcrypt": "^5.1.1",
      "compression": "^1.8.0",
      "cookie-parser": "^1.4.7",
      "cors": "^2.8.5",
      "dotenv": "^16.4.7",
      "express": "^5.1.0",
      "express-unless": "^2.1.3",
      "glob": "^11.0.1",
      "helmet": "^8.1.0",
      "ioredis": "^5.6.0",
      "jsonwebtoken": "^9.0.2",
      "morgan": "^1.10.0",
      "mysql2": "^3.14.0",
      "nodemailer": "^6.10.0",
      "passport": "^0.7.0",
      "passport-jwt": "^4.0.1",
      "pg": "^8.14.1",
      "pino": "^9.6.0",
      "pino-http": "^10.4.0",
      "redis": "^4.7.0",
      "reflect-metadata": "^0.2.2",
      "swagger-jsdoc": "^6.2.8",
      "swagger-ui-express": "^5.0.1",
      "typeorm": "0.3.22",
      "yaml": "^2.7.1",
      "zod": "^3.24.2"
   },
   "devDependencies": {
      "@types/bcrypt": "^5.0.2",
      "@types/cors": "^2.8.17",
      "@types/express": "^5.0.1",
      "@types/helmet": "^0.0.48",
      "@types/ioredis": "^4.28.10",
      "@types/jsonwebtoken": "^9.0.9",
      "@types/node": "^22.14.0",
      "@types/nodemailer": "^6.4.17",
      "@types/passport": "^1.0.17",
      "@types/passport-jwt": "^4.0.1",
      "@types/supertest": "^6.0.3",
      "@types/swagger-jsdoc": "^6.0.4",
      "@types/swagger-ui-express": "^4.1.8",
      "@typescript-eslint/eslint-plugin": "^8.29.0",
      "@typescript-eslint/parser": "^8.29.0",
      "eslint": "^9.23.0",
      "eslint-config-prettier": "^10.1.1",
      "eslint-plugin-prettier": "^5.2.6",
      "pino-pretty": "^13.0.0",
      "prettier": "^3.5.3",
      "supertest": "^7.1.0",
      "ts-node": "^10.9.2",
      "ts-node-dev": "^2.0.0",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.8.2",
      "vite-tsconfig-paths": "^5.1.4",
      "vitest": "^3.1.1"
   }
}
